{
  "name": "ac-scripts",
  "version": "1.0.0",
  "description": "CLI tool for setting up development environment",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "start": "node dist/index.js",
    "lint": "eslint src --fix",
    "prebuild": "npm run lint",
    "build": "webpack",
    "test": "jest"
  },
  "keywords": [],
  "author": "ndhung",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/banhmisg9509/acScripts"
  },
  "dependencies": {
    "enquirer": "^2.3.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/node": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "file-loader": "^6.2.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          }
        }
      ]
    ],
    "env": {
      "debug": {
        "sourceMaps": "inline",
        "retainLines": true
      }
    }
  }
}
